<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"   
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hand.mapper.CustomerMapper">
	
	<insert id="insert" parameterType="com.hand.dto.Customer">
		INSERT INTO customer (store_id,first_name,last_name,email,address_id,create_date) VALUES
		(1,#{first_name},#{last_name},#{email},#{address_id},#{create_date})
	</insert>

</mapper>